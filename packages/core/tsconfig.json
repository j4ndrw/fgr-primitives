{
    "compilerOptions": {
        "downlevelIteration": true,
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": false,
        "sourceMap": true,
        "target": "es5",
        "outDir": "dist",
        "skipLibCheck": true,
        "noUncheckedIndexedAccess": true,
        "strictNullChecks": true
    },
    "include": ["src/**/*"],
}
